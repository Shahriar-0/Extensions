{
    "manifest_version": 3,
    "name": "Copy Links",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "clipboardWrite"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Copy Links",
        "default_popup": "popup.html"
    }
}